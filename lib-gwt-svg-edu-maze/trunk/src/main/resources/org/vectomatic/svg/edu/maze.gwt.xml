<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name="com.google.gwt.core.Core" />
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.dom.DOM' />
	<inherits name="org.vectomatic.libgwtsvg" />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

	<inherits name="com.google.gwt.i18n.I18N" />
	<extend-property name="locale" values="fr" />

	<!-- Specify the app entry point class.                         -->
	<entry-point class='org.vectomatic.svg.edu.client.maze.MazeMain' />

	<!-- By default use the HttpRequestXmlLoader loader -->
	<replace-with class="org.vectomatic.svg.edu.client.commons.HttpRequestXmlLoader">
		<when-type-is class="org.vectomatic.svg.edu.client.commons.AsyncXmlLoader" />
	</replace-with>

</module>
